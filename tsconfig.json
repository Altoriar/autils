{
	"compilerOptions": {
		"outDir": "dist",
		"module": "esnext",
		"target": "esnext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"types": ["node"],
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"strict": true,
		"skipLibCheck": true,
		"jsx": "react-jsx"
	}
}
